   "source": [
    "n = int(input(\"Enter a number to check if it is a prime number: \"))\n",
    "\n",
    "count = 0\n",
    "\n",
    "for i in range(1, n+1):\n",
    "    if n % i == 0:\n",
    "        count += 1\n",
    "        \n",
    "if (n == 0) or (n == 1) or (count > 2):\n",
    "    print(n, \"is not a prime number.\")\n",
    "else:\n",
    "    print(n, \"is a prime number.\")"
   ]
  }
