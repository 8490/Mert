    "n = 100\n",
    "\n",
    "numbers = list(range(8, n))\n",
    "prime = [2, 3, 5, 7]\n",
    "\n",
    "for i in numbers:\n",
    "    if i%2==0 or i%3==0 or i%5==0 or i%7==0:\n",
    "        prime = prime\n",
    "    else:\n",
    "        prime += [i]\n",
    "    \n",
    "\n",
    "print(prime)"
   ]
